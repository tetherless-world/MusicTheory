@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix uo: <http://purl.obolibrary.org/obo/UO_> . 
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix mto-kb: <http://purl.org/ontology/mto/kb/> .
@prefix mto: <http://purl.org/ontology/mto/> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix tl: <http://purl.org/NET/c4dm/timeline.owl#> .
@prefix chord: <http://purl.org/ontology/chord/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix event: <http://purl.org/NET/c4dm/event.owl#> .
@prefix : <http://purl.org/ontology/mto/> .

<http://purl.org/ontology/mto/mto.ttl>
    rdf:type owl:Ontology ;
    dct:creator "Sabbir M. Rashid"^^xsd:string ;
    dct:created "2018-05-15T12:00:00"^^xsd:dateTime ;
    dct:modified "2018-05-19T14:30:00"^^xsd:dateTime ;
    dct:contributor "David C. De Roure"^^xsd:string, "Deborah L. McGuinness"^^xsd:string;
    dct:description "The Music Theory Ontology was designed to be able to formally represent rudimentary concepts used to describe music. Included are classes for musical notation, such as accidentals or time signatures; duration, describing how long a note is or is not played; intervals, representing tonal difference between pitches; and progressions, which involve the succession of notes or chords."^^xsd:string ;
    dct:title "Music Theory Ontology"^^xsd:string ;
    #owl:imports <http://purl.org/ontology/mo/>, <http://semanticscience.org/resource/>, <http://purl.org/NET/c4dm/timeline.owl#>, <http://purl.org/ontology/chord/> ;
    dct:license <http://www.apache.org/licenses/LICENSE-2.0> ;
    owl:versionIRI <http://purl.org/ontology/mto/0.9> .

:hasSemitoneCount rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of half steps or semitones associated with a given interval"^^xsd:string ;
    rdfs:label "has semitone count" ;
    rdfs:domain :Interval ;
    rdfs:range xsd:integer .

:hasNoteCount rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of notes or pitches associated with a given chord or scale"^^xsd:string ;
    rdfs:label "has note count" ;
    rdfs:range xsd:integer .

:hasFlatCount rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of flats associated with a given key"^^xsd:string ;
    rdfs:label "has flat count" ;
    rdfs:domain :KeySignature ;
    rdfs:range xsd:integer .

:hasSharpCount rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of sharps associated with a given key"^^xsd:string ;
    rdfs:domain :KeySignature ;
    rdfs:label "has sharp count" ;
    rdfs:range xsd:integer .

:hasTonic rdf:type owl:ObjectProperty ;
    rdfs:comment "Property to assign the tonic note associated with a given scale or chord"^^xsd:string ;
    rdfs:label "has tonic" ;
    owl:equivalentProperty :hasRootNote .

:hasDiatonicDegree rdf:type owl:ObjectProperty ;
    rdfs:comment "Property to assign the degree associated with a diatonic scale"^^xsd:string ;
    rdfs:label "has diatonic degree" ;
    rdfs:domain :DiatonicScale .

:hasDominant rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the dominant note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has dominant" ;
    rdfs:domain :DiatonicScale .

:hasMediant rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the mediant note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has mediant" ;
    rdfs:domain :DiatonicScale .

:hasSubdominant rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the subdominant note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has subdominant" ;
    rdfs:domain :DiatonicScale .

:hasSubmediant rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the submediant note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has submediant" ;
    rdfs:domain :DiatonicScale .

:hasSubtonic rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the subtonic note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has subtonic" ;
    rdfs:domain :DiatonicScale .

:hasSupertonic rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :hasDiatonicDegree ;
    rdfs:comment "Property to assign the supertonic note associated with a given diatonic scale"^^xsd:string ;
    rdfs:label "has supertonic" ;
    rdfs:domain :DiatonicScale .

:hasDegree rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign degree of a note in a scale or chord"^^xsd:string ;
    rdfs:label "has degree" ;
    rdfs:domain :Note ;
    rdfs:range xsd:integer .

:hasNote rdf:type owl:ObjectProperty ;
    rdfs:comment "Property to assign a note of arbitary degree to a chord or scale"^^xsd:string ;
    rdfs:label "has note" ;
    rdfs:range :Note .

:hasRootNote rdf:type owl:ObjectProperty ;
    rdfs:comment "Property to the root note to a chord or scale"^^xsd:string ;
    rdfs:label "has root note" ;
    rdfs:range :Note ;
    owl:equivalentProperty :hasTonic .

:hasUpperNumeral rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of beats associated with the upper numeral of a time signature, representing the beats per measure"^^xsd:string ;
    rdfs:label "has upper numeral" ;
    rdfs:domain :TimeSignature .

:hasLowerNumeral rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the number of beats associated with the lower numeral of a time signature, representing the time value of each beat"^^xsd:string ;
    rdfs:label "has lower numeral" ;
    rdfs:domain :TimeSignature .

:hasDurationLength  rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to assign the length of a duration, where a single unit represents a sixteenth of a beat"^^xsd:string ;
    rdfs:label "has duration length" ;
    rdfs:domain :Duration ;
    rdfs:range xsd:integer .

:hasIntervalPattern rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to represent pattern of intervals"^^xsd:string ;
    rdfs:label "has interval pattern" ;
    rdfs:domain :Scale .

:hasComponentsList rdf:type owl:DatatypeProperty ;
    rdfs:comment "Property to represent components of a chord in terms of intervals from the root note"^^xsd:string ;
    rdfs:label "has components list" ;
    rdfs:domain :Chord .

:MusicalNotation rdf:type owl:Class ;
    rdfs:comment "Symbol used to represent a musical concept"^^xsd:string ;
    rdfs:label "Musical Notation" .

:Stave rdf:type owl:Class ;
    rdfs:subClassOf :MusicalNotation ;
    rdfs:comment "A set of five parallel, equally-spaced, lines and four spaces, each representing a different musical pitch"^^xsd:string ;
    skos:altLabel "Staff"^^xsd:string ;
    rdfs:label "Stave"^^xsd:string .

:Bar rdf:type owl:Class ;
    rdfs:subClassOf :MusicalNotation ;
    rdfs:comment "A segment of time on a sheet of music corresponding to a specific number of beats"^^xsd:string ;
    skos:altLabel "Measure"^^xsd:string ;
    rdfs:label "Bar"^^xsd:string .

:Clef rdf:type owl:Class ;
    rdfs:subClassOf :MusicalNotation ;
    rdfs:comment "A musical symbol used to indicate the pitch of notes on a stave"^^xsd:string ;
    rdfs:label "Clef"^^xsd:string .

:GClef rdf:type owl:Class ;
    rdfs:subClassOf :Clef ;
    rdfs:comment "A clef with reference note G4"^^xsd:string ;
    rdfs:label "G-Clef"^^xsd:string .

:CClef rdf:type owl:Class ;
    rdfs:subClassOf :Clef ;
    rdfs:comment "A clef with reference note C4"^^xsd:string ;
    rdfs:label "C-Clef"^^xsd:string .

:FClef rdf:type owl:Class ;
    rdfs:subClassOf :Clef ;
    rdfs:comment "A clef with reference note F3"^^xsd:string ;
    rdfs:label "F-Clef"^^xsd:string .

:Signature rdf:type owl:Class ;
    rdfs:subClassOf :MusicalNotation ;
    rdfs:comment "A notational convention"^^xsd:string ;
    rdfs:label "Signature"^^xsd:string .

:KeySignature rdf:type owl:Class ;
    rdfs:comment "A set of accidentals placed on a stave to determine which notes are flat, natural, or sharp"^^xsd:string ;
    rdfs:subClassOf :Signature ;
    rdfs:label "Key Signature"^^xsd:string .

:TimeSignature rdf:type owl:Class ;
    rdfs:comment "A notational convention comprised of an upper and a lower numeral used to signify the number of beats per measure and time value of each beat, respectively"^^xsd:string ;
    rdfs:subClassOf :Signature ;
    rdfs:label "Time Signature"^^xsd:string .

:SimpleMeter rdf:type owl:Class ;
    rdfs:comment "A time signature where the beat can be broken down into two-part rhythms"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Simple Meter"^^xsd:string .

:CompoundMeter rdf:type owl:Class ;
    rdfs:comment "A time signature where the beat can be broken down into three-part rhythms"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Compound Meter"^^xsd:string .

:ComplexMeter rdf:type owl:Class ;
    rdfs:comment "A time signature where the beat is irregular in the sense that it cannot be broken down into two- or three-part rhythms"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Complex Meter"^^xsd:string .

:MixedMeter rdf:type owl:Class ;
    rdfs:comment "A notational convention representing multiple, often alternating time signatures"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Mixed Meter"^^xsd:string .

:AdditiveMeter rdf:type owl:Class ;
    rdfs:comment "A time signature where the beat is subdivided into smaller, irregular groups"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Additive Meter"^^xsd:string .

:FractionalMeter rdf:type owl:Class ;
    rdfs:comment "A time signature that has an upper numeral that is a fraction"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Fractional Meter"^^xsd:string .

:IrrationalMeter rdf:type owl:Class ;
    rdfs:comment "A time signature that has a lower numeral that is not a power of 2"^^xsd:string ;
    rdfs:subClassOf :TimeSignature ;
    rdfs:label "Irrational Meter"^^xsd:string .

:Interval rdf:type owl:Class ;
    rdfs:comment "The difference between two pitches"^^xsd:string ;
    owl:equivalentClass chord:Interval ;
    rdfs:label "Interval"^^xsd:string .

:HarmonicInterval rdf:type owl:Class ;
    rdfs:subClassOf :Interval ;
    rdfs:comment "Occurs when multiple notes are played at the same time"^^xsd:string ;
    rdfs:label "Harmonic Interval"^^xsd:string .

:Chord rdf:type owl:Class ;
    rdfs:subClassOf :HarmonicInterval ;
    owl:equivalentClass chord:Chord ;
    rdfs:comment "Any harmonic set of pitches consisting of two or more notes sounded simultaneously"^^xsd:string ;
    rdfs:label "Chord"^^xsd:string .

:Monad rdf:type owl:Class ;
    rdfs:subClassOf :Chord ;
    rdfs:comment "A single note or pitch"^^xsd:string ;
    :hasNoteCount "1"^^xsd:integer ;
    rdfs:label "Monad"^^xsd:string ;
    skos:altLabel "Monochord"^^xsd:string .

:Dyad rdf:type owl:Class ;
    rdfs:comment "A set of two notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "2"^^xsd:integer ;
    rdfs:label "Dyad"^^xsd:string ;
    skos:altLabel "Dichord"^^xsd:string .

:Triad rdf:type owl:Class ;
    rdfs:comment "A set of three notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "3"^^xsd:integer ;
    rdfs:label "Triad"^^xsd:string ;
    skos:altLabel "Trichord"^^xsd:string .

:MajorTriad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, and a perfect fifth above the root"^^xsd:string ;
    rdfs:subClassOf :Triad ;
    :hasComponentsList "(3,5)"^^xsd:string ;
    skos:altLabel "maj"^^xsd:string ;
    rdfs:label "Major Triad"^^xsd:string .

:MinorTriad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, and a perfect fifth above the root"^^xsd:string ;
    rdfs:subClassOf :Triad ;
    :hasComponentsList "(b3,5)"^^xsd:string ;
    skos:altLabel "min"^^xsd:string ;
    rdfs:label "Minor Triad"^^xsd:string .

:AugmentedTriad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, and a note an augmented fifth above the root"^^xsd:string ;
    rdfs:subClassOf :Triad ;
    :hasComponentsList "(3,#5)"^^xsd:string ;
    skos:altLabel "aug"^^xsd:string ;
    rdfs:label "Augmented Triad"^^xsd:string .

:DiminishedTriad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, and a diminished fifth above the root"^^xsd:string ;
    rdfs:subClassOf :Triad ;
    :hasComponentsList "(b3,b5)"^^xsd:string ;
    skos:altLabel "dim"^^xsd:string ;
    rdfs:label "Diminished Triad"^^xsd:string .

:SuspendedChord rdf:type owl:Class ;
    rdfs:comment "A chord in which the third is replaced"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    rdfs:label "Suspended Chord"^^xsd:string .

:SuspendedSecondTriad rdf:type owl:Class ;
    rdfs:comment "A chord in which the third is replaced by the major second"^^xsd:string ;
    rdfs:subClassOf :SuspendedChord, :Triad ;
    :hasComponentsList "(2,5)"^^xsd:string ;
    skos:altLabel "sus2"^^xsd:string ;
    rdfs:label "Suspended Second Triad"^^xsd:string .

:SuspendedFourthTriad rdf:type owl:Class ;
    rdfs:comment "A chord in which the third is replaced by the perfect fourth"^^xsd:string ;
    rdfs:subClassOf :SuspendedChord, :Triad ;
    :hasComponentsList "(4,5)"^^xsd:string ;
    skos:altLabel "sus4"^^xsd:string ;
    rdfs:label "Suspended Fourth Triad"^^xsd:string .

:Tetrad rdf:type owl:Class ;
    rdfs:comment "A set of four notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "4"^^xsd:integer ;
    rdfs:label "Tetrad"^^xsd:string ;
    skos:altLabel "Tetrachord"^^xsd:string .

:SixthTetrad rdf:type owl:Class ;
    rdfs:comment "A chord in which the sixth above the root is also included"^^xsd:string ;
    rdfs:subClassOf :Tetrad ;
    rdfs:label "Sixth Tetrad"^^xsd:string .

:MajorSixthTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, a perfect fifth above the root, and a major 6th above the root"^^xsd:string ;
    rdfs:subClassOf :SixthTetrad ;
    :hasComponentsList "(3,5,6)"^^xsd:string ;
    skos:altLabel "maj6"^^xsd:string ;
    rdfs:label "Major Sixth Tetrad"^^xsd:string .

:MinorSixthTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a perfect fifth above the root, and a major 6th above the root"^^xsd:string ;
    rdfs:subClassOf :SixthTetrad ;
    :hasComponentsList "(b3,5,6)"^^xsd:string ;
    skos:altLabel "min6"^^xsd:string ;
    rdfs:label "Minor Sixth Tetrad"^^xsd:string .

:SeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord in which the seventh above the root is also included"^^xsd:string ;
    rdfs:subClassOf :Tetrad ;
    rdfs:label "Seventh Tetrad"^^xsd:string .

:MajorSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, a perfect fifth above the root, and a major 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(3,5,7)"^^xsd:string ;
    skos:altLabel "maj7"^^xsd:string ;
    rdfs:label "Major Seventh Chord"^^xsd:string .

:MinorSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a perfect fifth above the root, and a minor 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(b3,5,b7)"^^xsd:string ;
    skos:altLabel "min7"^^xsd:string ;
    rdfs:label "Minor Seventh Tetrad"^^xsd:string .

:DominantSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, a perfect fifth above the root, and a minor 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(3,5,b7)"^^xsd:string ;
    skos:altLabel "7"^^xsd:string ;
    rdfs:label "Dominant Seventh Tetrad"^^xsd:string .

:DiminishedSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a diminished fifth above the root, and a diminished minor 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(b3,b5,bb7)"^^xsd:string ;
    skos:altLabel "dim7"^^xsd:string ;
    rdfs:label "Diminished Seventh Tetrad"^^xsd:string .

:HalfDiminishedSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a diminished fifth above the root, and a minor 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(b3,b5,b7)"^^xsd:string ;
    skos:altLabel "dim7"^^xsd:string ;
    rdfs:label "Half Diminished Seventh Tetrad"^^xsd:string .

:MinorMajorSeventhTetrad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a perfect fifth above the root, and a major 7th above the root"^^xsd:string ;
    rdfs:subClassOf :SeventhTetrad ;
    :hasComponentsList "(b3,5,7)"^^xsd:string ;
    skos:altLabel "minmaj7"^^xsd:string ;
    rdfs:label "Minor (Major Seventh) Tetrad"^^xsd:string .

:Pentad rdf:type owl:Class ;
    rdfs:comment "A set of five notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "5"^^xsd:integer ;
    rdfs:label "Pentad"^^xsd:string ;
    skos:altLabel "Pentachord"^^xsd:string .

:NinthPentad rdf:type owl:Class ;
    rdfs:comment "A chord in which the seventh and ninth above the root are included"^^xsd:string ;
    rdfs:subClassOf :Pentad ;
    rdfs:label "Ninth Pentad"^^xsd:string .

:MajorNinthPentad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, a perfect fifth above the root, a major 7th above the root, and a major 9th above the root"^^xsd:string ;
    rdfs:subClassOf :NinthPentad ;
    :hasComponentsList "(3,5,7,9)"^^xsd:string ;
    skos:altLabel "maj9"^^xsd:string ;
    rdfs:label "Major Ninth Pentad"^^xsd:string .

:MinorNinthPentad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a minor third above the root, a perfect fifth above the root, and a minor 7th above the root, and a major 9th above the root"^^xsd:string ;
    rdfs:subClassOf :NinthPentad ;
    :hasComponentsList "(b3,5,b7,9)"^^xsd:string ;
    skos:altLabel "min9"^^xsd:string ;
    rdfs:label "Minor Ninth Pentad"^^xsd:string .

:DominantNinthPentad rdf:type owl:Class ;
    rdfs:comment "A chord consisting of a root note, a major third above the root, a perfect fifth above the root, and a minor 7th above the root, and a major 9th above the root"^^xsd:string ;
    rdfs:subClassOf :NinthPentad ;
    :hasComponentsList "(3,5,b7,9)"^^xsd:string ;
    skos:altLabel "9"^^xsd:string ;
    rdfs:label "Dominant Ninth Pentad"^^xsd:string .

:Hexad rdf:type owl:Class ;
    rdfs:comment "A set of six notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "6"^^xsd:integer ;
    rdfs:label "Hexad"^^xsd:string ;
    skos:altLabel "Hexachord"^^xsd:string .

:Heptad rdf:type owl:Class ;
    rdfs:comment "A set of seven notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "7"^^xsd:integer ;
    rdfs:label "Heptad"^^xsd:string ;
    skos:altLabel "Heptachord"^^xsd:string .

:Octad rdf:type owl:Class ;
    rdfs:comment "A set of eight notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "8"^^xsd:integer ;
    rdfs:label "Octad"^^xsd:string ;
    skos:altLabel "Octachord"^^xsd:string .

:Ennead rdf:type owl:Class ;
    rdfs:comment "A set of nine notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "9"^^xsd:integer ;
    rdfs:label "Ennead"^^xsd:string ;
    skos:altLabel "Nonachord"^^xsd:string .

:Decad rdf:type owl:Class ;
    rdfs:comment "A set of ten notes or pitches"^^xsd:string ;
    rdfs:subClassOf :Chord ;
    :hasNoteCount "10"^^xsd:integer ;
    rdfs:label "Decad"^^xsd:string ;
    skos:altLabel "Decachord"^^xsd:string .

:MelodicInterval rdf:type owl:Class ;
    rdfs:subClassOf :Interval ;
    rdfs:comment "Occurs when multiple notes are played in sequence"^^xsd:string ;
    rdfs:label "Melodic Interval"^^xsd:string .

:DiatonicInterval rdf:type owl:Class ;
    rdfs:comment "Any interval where the first and last note are part of the same major scale"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Diatonic Interval"^^xsd:string .

:ChromaticInterval rdf:type owl:Class ;
    rdfs:comment "Any interval where the first and last note are not part of the same major scale"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Chromatic Interval"^^xsd:string .

:PerfectInterval rdf:type owl:Class ;
    rdfs:comment "An interval with a high degree of consonance (traditionally considered perfectly consonant) that can be inverted to form other perfect intervals"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Perfect Interval"^^xsd:string .

:MajorInterval rdf:type owl:Class ;
    rdfs:comment "An interval corresponding to notes in a major scale"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Major Interval"^^xsd:string .

:MinorInterval rdf:type owl:Class ;
    rdfs:comment "An interval in which the corresponding major interval is narrowed by a chromatic semitone"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Minor Interval"^^xsd:string .

:AugmentedInterval rdf:type owl:Class ;
    rdfs:comment "An interval in which a major or perfect interval is widened by a chromatic semitone"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Augmented Interval"^^xsd:string .

:DiminishedInterval rdf:type owl:Class ;
    rdfs:comment "An interval in which a minor or perfect interval is narrowed by a chromatic semitone"^^xsd:string ;
    rdfs:subClassOf :Interval ;
    rdfs:label "Diminished Interval"^^xsd:string .

:PerfectUnisonInterval rdf:type owl:Class ;
    rdfs:comment "An interval where two or more notes are playing the same pitch"^^xsd:string ;
    :hasSemitoneCount "0"^^xsd:integer ;
    rdfs:subClassOf :PerfectInterval ;
    rdfs:label "Perfect Unison"^^xsd:string ;
    skos:altLabel "P1"^^xsd:string ;
    owl:equivalentClass :DiminishedSecondInterval .

:DiminishedSecondInterval rdf:type owl:Class ;
    rdfs:comment "The interval produced by diminishing a minor second by one chromatic semitone, enharmonically equivalent to a perfect unison"^^xsd:string ;
    :hasSemitoneCount "0"^^xsd:integer ;
    rdfs:subClassOf :DiminishedInterval ;
